+static unsigned int am335x_pin_mux_addr_to_skip[] = {
+	0x9bc,
+	0x9c4, 0x9c8, 0x9cc,
+	0x9ec,
+	0x9f0, 0x9f4,
+	0xa08, 0xa0c,
+	0xa10, 0xa14, 0xa18,
+	0xa20, 0xa24, 0xa28, 0xa2c,
+	0xa30,
